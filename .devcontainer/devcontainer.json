{
    "name": "matlab",
    "dockerComposeFile": "compose.yml",
    "service": "matlab",
    "workspaceFolder": "/workspace",
    "hostRequirements": {
        "memory": "4gb"
    },
    "remoteEnv": {
        "THE_SECRET": "${localEnv:THE_SECRET}",
        "MATLAB_MAC": "${localEnv:MATLAB_MAC}"
    },
    "containerEnv": {
        "THE_SECRET": "${localEnv:THE_SECRET}",
        "MATLAB_MAC": "${localEnv:MATLAB_MAC}"
    },
    "onCreateCommand": "/home/matlab/setup.sh $THE_SECRET",
    "customizations": {
        "vscode": {
            "extensions": [
                "bat67.matlab-extension-pack"
            ]
        }
    }
}